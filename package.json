{
    "name": "amoscato-client",
    "license": "MIT",
    "version": "0.1.0",
    "description": "Front end client for amoscato.com",
    "private": true,
    "scripts": {
        "start": "concurrently --raw \"npm:start-*\"",
        "start-hugo": "hugo serve",
        "start-gulp": "gulp",
        "test": "karma start --single-run",
        "posttest": "npm run lint-js && npm run prettier",
        "predeploy": "gulp all && hugo --minify",
        "deploy": "gulp deploy",
        "lint-js": "eslint . --ext .js,.ts",
        "lint-sass": "stylelint \"assets/**/*.scss\"",
        "prettier": "prettier --check ."
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/namoscato/amoscato-client.git"
    },
    "dependencies": {
        "core-js": "^3.15.2",
        "jquery": "^3.6.0",
        "moment": "^2.29.1",
        "onecolor": "^3.1.0"
    },
    "devDependencies": {
        "@babel/core": "^7.14.6",
        "@babel/plugin-proposal-class-properties": "^7.14.5",
        "@babel/preset-env": "^7.14.7",
        "@babel/preset-typescript": "^7.14.5",
        "@types/jasmine": "^3.8.1",
        "@types/jquery": "^3.5.6",
        "@types/moment": "^2.13.0",
        "@typescript-eslint/eslint-plugin": "^4.28.3",
        "@typescript-eslint/parser": "^4.28.3",
        "babel-loader": "^8.2.2",
        "concurrently": "^6.2.0",
        "config": "^3.3.6",
        "eslint": "^7.30.0",
        "eslint-config-prettier": "^8.3.0",
        "fancy-log": "^1.3.3",
        "gulp": "^4.0.2",
        "jasmine-core": "^3.8.0",
        "karma": "^6.3.4",
        "karma-chrome-launcher": "^3.1.0",
        "karma-jasmine": "^4.0",
        "karma-webpack": "^5.0.0",
        "prettier": "2.3.2",
        "stylelint": "^13.13.1",
        "stylelint-config-prettier": "^8.0.2",
        "stylelint-config-sass-guidelines": "^8.0.0",
        "typescript": "^4.3.5",
        "vinyl-ftp": "^0.6.1",
        "webpack": "^5.46.0",
        "webpack-stream": "^6.1.2"
    },
    "engines": {
        "node": ">=10"
    }
}
