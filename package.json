{
    "name": "amoscato-client",
    "license": "MIT",
    "version": "0.1.0",
    "description": "Front end client for amoscato.com",
    "private": true,
    "scripts": {
        "start": "hugo serve",
        "test": "karma start --single-run",
        "posttest": "npm run lint-js && npm run prettier && npm run lint-sass",
        "predeploy": "hugo --minify",
        "deploy": "gulp deploy",
        "lint-js": "eslint . --ext .js,.ts",
        "lint-sass": "stylelint \"assets/**/*.scss\"",
        "prettier": "prettier --check ."
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/namoscato/amoscato-client.git"
    },
    "dependencies": {
        "core-js": "^3.19.2",
        "jquery": "^3.6.0",
        "moment": "^2.29.1",
        "onecolor": "^3.1.0"
    },
    "devDependencies": {
        "@babel/core": "^7.16.0",
        "@babel/preset-env": "^7.15.0",
        "@babel/preset-typescript": "^7.16.0",
        "@types/jasmine": "^3.8.2",
        "@types/jquery": "^3.5.9",
        "@types/moment": "^2.13.0",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/parser": "^4.33.0",
        "babel-loader": "^8.2.3",
        "config": "^3.3.6",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "fancy-log": "^1.3.3",
        "gulp": "^4.0.2",
        "jasmine-core": "^3.10.1",
        "karma": "^6.3.9",
        "karma-chrome-launcher": "^3.1.0",
        "karma-jasmine": "^4.0",
        "karma-webpack": "^5.0.0",
        "prettier": "2.5.0",
        "stylelint": "^13.13.1",
        "stylelint-config-prettier": "^8.0.2",
        "stylelint-config-sass-guidelines": "^8.0.0",
        "typescript": "^4.3.5",
        "vinyl-ftp": "^0.6.1",
        "webpack": "^5.64.4"
    },
    "engines": {
        "node": ">=10"
    }
}
